{
  "name": "Herbs",
  "rs2fPath": "module.rs2f",
  "inputs": [
    {
      "type": "enumlist",
      "macroName": "VAR_HERB_SHOW",
      "label": "Herbs to always show regardless of value",
      "enum": [
        "guam leaf",
        "marrentill",
        "tarromin",
        "harralander",
        "ranarr weed",
        "toadflax",
        "irit leaf",
        "avantoe",
        "kwuarm",
        "huasca",
        "snapdragon",
        "cadantine",
        "lantadyme",
        "dwarf weed",
        "torstol"
      ],
      "default": []
    },
    {
      "type": "enumlist",
      "macroName": "VAR_HERB_HIDE",
      "label": "Herbs to always hide regardless of value",
      "enum": [
        "guam leaf",
        "marrentill",
        "tarromin",
        "harralander",
        "ranarr weed",
        "toadflax",
        "irit leaf",
        "avantoe",
        "kwuarm",
        "huasca",
        "snapdragon",
        "cadantine",
        "lantadyme",
        "dwarf weed",
        "torstol"
      ],
      "default": ["guam leaf", "marrentill", "tarromin"]
    },
    {
      "type": "style",
      "macroName": "VAR_HERB_CUSTOMSTYLE",
      "label": "Style",
      "default": {
        "textColor": "#00ff00"
      }
    },
    {
      "type": "number",
      "macroName": "VAR_HERB_MINVALUE",
      "label": "Baseline minimum value at which to show herbs.",
      "default": 1
    }
  ]
}
